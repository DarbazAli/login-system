<body>
  <%- include('header') %>


  <h1>Register</h1>

  <form action="/register" method="POST">
    <div>
        <label for="name">name</label>
        <input type="text" name="name" id="name" required /><br />
    
        <label for="email">Email</label>
        <input type="email" name="email" id="email" required /><br />
    
